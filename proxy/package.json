{
  "name": "singlelink-proxy",
  "version": "2.1.2",
  "description": "Proxy for singlelink",
  "main": "dist/App.js",
  "scripts": {
    "copy": "(copyfiles -f \"src/sql/*.sql\" dist/sql) && (copyfiles -f src/crypto/*.* dist/crypto/)",
    "start": "node dist/App.js",
    "build": "tsc && npm run copy",
    "dev": "ts-node --files src/App.ts --no-cache",
    "test": "jest",
    "test-debug": "jest --runInBand --detectOpenHandles"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "author": "Neutron Creative Inc.",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Neutron-Creative/Singlelink/issues"
  },
  "homepage": "https://github.com/Neutron-Creative/Singlelink#readme",
  "dependencies": {
    "redbird": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.6",
    "@types/pg": "^7.14.5",
    "@types/stripe": "^7.13.25",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.6.1",
    "copyfiles": "^2.4.0",
    "eslint": "^7.12.1",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
